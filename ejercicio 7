          -- [(CiudadO, CiudadD, Dur)] (de origen)
puedoVolverAOrigen :: AgenciaDeViajes -> Ciudad -> Bool
puedoVolverAOrigen [] _ = False
puedoVolverAOrigen ((x:xs):(y:ys):(w:ws)) Ciudad | (x == head ys) == Ciudad = vuelosValidos ..
